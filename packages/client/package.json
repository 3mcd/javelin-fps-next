{
  "name": "javelin-fps-client",
  "private": true,
  "version": "0.0.1",
  "description": "Javelin FPS client",
  "types": "src/index.js",
  "main": "dist/index.js",
  "scripts": {
    "build:src": "tsc -b",
    "build:bundle": "node -r esbuild-register scripts/build.ts --build",
    "build": "run-s build:*",
    "watch:src": "run-p \"build:src -- -w\" \"build:bundle -- --watch\"",
    "watch": "run-s build:src watch:src",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Eric McDaniel <eric.g.mcdaniel@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@dimforge/rapier3d-module": "workspace:*",
    "@javelin/core": "^1.0.0-alpha.8",
    "@javelin/ecs": "1.0.0-alpha.8",
    "@javelin/net": "1.0.0-alpha.9",
    "@types/live-server": "^1.2.0",
    "@types/minimist": "^1.2.1",
    "contro": "^2.2.0",
    "javelin-fps-shared": "workspace:^0.0.1",
    "live-server": "^1.2.1",
    "minimist": "^1.2.5",
    "three": "^0.129.0"
  },
  "devDependencies": {
    "@types/three": "^0.129.1"
  }
}